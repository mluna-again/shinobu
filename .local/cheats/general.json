{
  "Lowercase string (stdin)": "tr '[:upper:]' '[:lower:]'",
  "Uppercase string (stdin)": "tr '[:lower:]' '[:upper:]'",
  "Lowercase string (inline)": "tr '[:upper:]' '[:lower:]' <<< '<your text here>'",
  "Uppercase string (inline)": "tr '[:lower:]' '[:upper:]' <<< '<your text here>'",
  "Terminal height": "tput lines",
  "Terminal width": "tput cols",
  "Hide cursor": "printf '\\033[?25l'",
  "Show cursor": "printf '\\033[?25h'"
}
