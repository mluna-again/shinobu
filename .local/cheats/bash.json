{
  "Lowercase string (stdin)": "tr '[:upper:]' '[:lower:]'",
  "Uppercase string (stdin)": "tr '[:lower:]' '[:upper:]'",
  "Lowercase string (inline)": "tr '[:upper:]' '[:lower:]' <<< '<your text here>'",
  "Uppercase string (inline)": "tr '[:lower:]' '[:upper:]' <<< '<your text here>'",
  "Terminal height": "tput lines",
  "Terminal width": "tput cols",
  "Hide cursor": "printf '\\033[?25l'",
  "Show cursor": "printf '\\033[?25h'",
  "Reset colors": "printf '\\033[0m'",
  "Longest line (count)": "awk '{ if ( length > x ) { x = length } } END { print x }'",
  "Longest line (text)": "awk '{ if ( length > x ) { x = length; line = $0 } } END { print line }'",
  "Enter container": "docker container exec -it (dock id) bash",
  "Files older than": "find . -type f -mtime +<days>",
  "PostgreSQL connections string": "postgresql://postgres:postgres@localhost:5432/database",
  "Print red": "printf '\\033[0;31m%s\\n\\033[0m' 'hello'",
  "Print yellow": "printf '\\033[0;33m%s\\n\\033[0m' 'hello'",
  "Print green": "printf '\\033[0;32m%s\\n\\033[0m' 'hello'",
  "Print blue": "printf '\\033[0;34m%s\\n\\033[0m' 'hello'",
  "Print orange": "printf '\\033[0;33m%s\\n\\033[0m' 'hello'",
  "Print purple": "printf '\\033[0;35m%s\\n\\033[0m' 'hello'",
  "Print red background": "printf '\\033[41;30m%s\\n\\033[0m' 'hello'",
  "Print yellow background": "printf '\\033[43;30m%s\\n\\033[0m' 'hello'",
  "Print green background": "printf '\\033[42;30m%s\\n\\033[0m' 'hello'",
  "Print blue background": "printf '\\033[44;30m%s\\n\\033[0m' 'hello'",
  "Print purple background": "printf '\\033[45;30m%s\\n\\033[0m' 'hello'"
}
