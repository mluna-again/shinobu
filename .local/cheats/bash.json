{
  "Lowercase string (stdin)": "tr '[:upper:]' '[:lower:]'",
  "Uppercase string (stdin)": "tr '[:lower:]' '[:upper:]'",
  "Lowercase string (inline)": "tr '[:upper:]' '[:lower:]' <<< '<your text here>'",
  "Uppercase string (inline)": "tr '[:lower:]' '[:upper:]' <<< '<your text here>'",
  "Terminal height": "tput lines",
  "Terminal width": "tput cols",
  "Hide cursor": "printf '\\033[?25l'",
  "Show cursor": "printf '\\033[?25h'",
  "Print error": "printf '\\033[0;30m\\033[41m %s \\n' \"\"",
  "Print success": "printf '\\033[0;30m\\033[42m %s \\n' \"\"",
  "Print warning": "printf '\\033[0;30m\\033[43m %s \\n' \"\"",
  "Reset colors": "printf '\\033[0m'",
  "Longest line (count)": "awk '{ if ( length > x ) { x = length } } END { print x }'",
  "Longest line (text)": "awk '{ if ( length > x ) { x = length; line = $0 } } END { print line }'",
  "Enter container": "docker container exec -it (dock id) bash",
  "Files older than": "find . -type f -mtime +<days>"
}
